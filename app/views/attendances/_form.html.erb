<%= form_for(@attendance,builder: BootstrapFormBuilder) do |f| %>

     <%= f.errors %><br>
    <%= f.label :gone %><br>
      <%= f.check_box :gone %>
    
      <%= f.number_field :fieldPrice, step: :any %>
    
      <%= f.number_field :fee, step: :any %>
    
    <%= f.label :Player %><br>
    <%= f.select(:player_id, options_for_select(@player)) %>
    
    <%= f.label :Saving %><br>
    <%= f.select(:saving_id, options_for_select(@saving)) %>
    
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
